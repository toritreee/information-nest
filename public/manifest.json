{
  "$schema": "https://json.schemastore.org/web-manifest-combined.json",
  "name": "information-nest",
  "short_name": "I-N",
  "start_url": "app",
  "display": "standalone",
  "background_color": "#fff",
  "description": "予定表",
  "icons": [
    {
      "src": "/icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    }
  ],
  "id":"/app",
  "orientation":"any",
  "theme_color":"#404189",
  "categories":["education","navigation","utilities"],
  "screenshots": [
    {
      "src": "icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "platform": "narrow"
    }
  ],
  "display_override": [
    "minimal-ui"
  ],
  "shortcuts": [
    {
      "name": "New Calender",
      "url": "/new/create",
      "description": "スケジュールを作成する"
    }
  ],
  "launch_handler":"focus-existing"
}